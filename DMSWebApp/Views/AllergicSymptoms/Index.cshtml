@model IEnumerable<DMSWebApp.Models.AllergicSymptom>

@{
    ViewData["Title"] = "Index";
}


<h1 class="text-white">Allergic Symptoms-View</h1>

<a class="btn btn-success" asp-action="Create">Allergic Symptoms Registration</a>
<table class="table text-white">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AllergicSymptomId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AllergyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
             <td>
                @Html.DisplayFor(modelItem => item.AllergicSymptomId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AllergyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                <a class="text-white" asp-action="Details" asp-route-id="@item.AllergicSymptomId">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
